    <script src="~/Areas/Admin/Contents/ContentLayout/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="~/Areas/Admin/Contents/ContentLayout/vendor/slick/slick.min.js">
    </script>
    <script src="~/Areas/Admin/Contents/ContentLayout/vendor/wow/wow.min.js"></script>
    <script src="~/Areas/Admin/Contents/ContentLayout/vendor/animsition/animsition.min.js"></script>
    <script src="~/Areas/Admin/Contents/ContentLayout/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="~/Areas/Admin/Contents/ContentLayout/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="~/Areas/Admin/Contents/ContentLayout/vendor/counter-up/jquery.counterup.min.js">
    </script>
    <script src="~/Areas/Admin/Contents/ContentLayout/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="~/Areas/Admin/Contents/ContentLayout/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/Areas/Admin/Contents/ContentLayout/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="~/Areas/Admin/Contents/ContentLayout/vendor/select2/select2.min.js">
    </script>

    <!-- Main JS-->
    <script src="~/Areas/Admin/Contents/ContentLayout/js/main.js"></script>
    
</body>

